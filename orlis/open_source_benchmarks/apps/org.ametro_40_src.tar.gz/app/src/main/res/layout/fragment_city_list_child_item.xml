<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/city_list_child_item_height"
    android:paddingLeft="@dimen/list_item_padding_left_right"
    android:paddingRight="@dimen/list_item_padding_left_right"
    >
    <TextView android:id="@+id/city"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:paddingLeft="?android:attr/expandableListPreferredItemPaddingLeft"
        android:paddingRight="?android:attr/expandableListPreferredItemPaddingLeft"
        style="@style/primaryText"
        tools:text="City list child name"
        />
</RelativeLayout>
