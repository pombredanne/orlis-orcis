    <string name="libs_section_title">Librerie Gratuite &amp; Open Source usate:</string>

    <string name="fab_desc">
		<![CDATA[
        <b>Floating Action Button</b> è una libreria per implementare e promuovere azioni nello stile Material Design.
        ]]></string>

    <string name="materialdialogs_desc">
		<![CDATA[
        <b>Material Dialogs</b> è una fantastica API, "facile da usare" e personalizzabile, per implementare dialoghi in stile Material Design su ogni versione di Android.
        ]]></string>

    <string name="materialdrawer_desc">
        <![CDATA[
        La libreria <b>MaterialDrawer</b> ha lo scopo di fornire la più semplice implementazione possibile di un navigation drawer per la tua applicazione.
        ]]>
    </string>

    <string name="picasso_desc">
        <![CDATA[
        <b>Picasso</b> è una potente libreria per lo scaricamento/caching di immagini per Android.
        ]]></string>

    <string name="okhttp_desc">
        <![CDATA[
        <b>OkHttp</b> è un client HTTP e SPDY per Android ed applicazioni Java.
        ]]></string>

    <string name="snackbar_desc">
        <![CDATA[
        <b>SnackBar</b> è una librerie per visualizzare toast seguendo le guidelinea del Material Design.
        ]]></string>

    <string name="crash_desc">
        <![CDATA[
        È una libreria Android che permette di lanciare una activity personalizzata quando la tua app crasha, invece di mostrare il tanto odiato dialog "Sfortunatamente, X si è bloccato".
        ]]></string>

    <string name="appintro_desc">
        <![CDATA[
        Libreria Android per creare fantastici intro per le tue app.
        ]]></string>

    <string name="materialripple_desc">
        <![CDATA[
        Fornisce l\'effetto Ripple sulle Views Android
        ]]></string>

    <string name="materialpreference_desc">
        <![CDATA[
        Semplice implementazione di una schermata delle preferenze in stile Material Design anche per telefoni pre-lollipop
        ]]></string>

    <string name="licensesdialog_desc">
        <![CDATA[
        LicensesDialog è una libreria per mostrare le licenze di librerie di terze parti in una app Android
        ]]></string>

