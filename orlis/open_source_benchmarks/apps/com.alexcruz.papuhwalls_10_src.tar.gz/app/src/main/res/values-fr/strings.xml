    <string name="libs_section_title">Bibliothèques FOSS utilisées :</string>

    <string name="fab_desc">
		<![CDATA[
        <b>Floating Action Button</b> est une bibliothèque pour la conception \"Material\" pour le bouton d\'action.
        ]]></string>

    <string name="materialdialogs_desc">
		<![CDATA[
        <b>Material Dialogs</b> est une API superbe, facile à utiliser, personnalisable et permettant une intégration de la conception \"Material\" dans toutes les versions d\'Android.
        ]]></string>

    <string name="materialdrawer_desc">
        <![CDATA[
        La bibliothèque <b>MaterialDrawer</b> a pour objectif de fournir la meilleure intégration du volet de navigation pour votre application.
        ]]>
    </string>

    <string name="picasso_desc">
        <![CDATA[
        <b>Picasso</b> est une bibliothèque puissante de téléchargement et de mise en cache d\'image pour Android.
        ]]></string>

    <string name="okhttp_desc">
        <![CDATA[
        <b>OkHttp</b> est un client HTTP et SPDY pour applications Android et Java.
        ]]></string>
 
    <string name="snackbar_desc">
        <![CDATA[
        <b>SnackBar</b> est un modèle de notification toast d\'Android inspiré des spécifications de la conception \"Material\" de Google.
        ]]></string>

    <string name="crash_desc">
        <![CDATA[
        Bibliothèque Android permettant de démarrer une activité lors du plantage de votre application, au lieu d\'afficher l\'horrible fenêtre de dialogue \"Malheureusement X s\'est arrêté\".
        ]]></string>

    <string name="appintro_desc">
        <![CDATA[
        Bibliothèque Android permettant de faire une jolie introduction pour votre application.
        ]]></string>

    <string name="materialripple_desc">
        <![CDATA[
        Bibliothèque Android permettant un effet d\'ondulations pour \"Android Views\".
        ]]></string>

    <string name="materialpreference_desc">
        <![CDATA[
        Intégration simple de la conception \"Material\" dans les paramètres pour toutes les versions d\'Android.
        ]]></string>

    <string name="licensesdialog_desc">
        <![CDATA[
        \"LicensesDialog\" est une bibliothèque \"Open Source\" pour afficher les licences des bibliothèques tierces dans une application Android.
        ]]></string>
    
