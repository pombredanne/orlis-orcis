apply plugin: 'com.android.application'
android {
lintOptions { abortOnError false }
    
    compileSdkVersion 23
    buildToolsVersion '23.0.2'
    defaultConfig {
        applicationId "de.tap.easy_xkcd"
        minSdkVersion 15
        targetSdkVersion 23
        versionCode 131
        versionName "5.3.2"
    }
    buildTypes {
        release {
minifyEnabled false
proguardFiles '/home/yan/Android/Sdk/tools/proguard/proguard-android.txt'
        }
    }

    repositories {
        mavenCentral()
        jcenter()
        maven {
            url "https://s3.amazonaws.com/repo.commonsware.com"
        }
        maven {
url "https://jitpack.io"
        }
    }
    lintOptions {
        disable 'InvalidPackage'
    }
    packagingOptions {
        exclude 'META-INF/services/javax.annotation.processing.Processor' // for realm
    }
}
dependencies {
    compile fileTree(dir: 'libs', include: ['*.jar'])
    compile 'com.android.support:design:23.1.1'
    compile 'com.github.bumptech.glide:glide:3.6.0'
    compile 'com.github.chrisbanes.photoview:library:1.2.3'
    compile 'com.android.support:recyclerview-v7:23.1.1'
    compile 'com.android.support:cardview-v7:23.1.1'
    compile 'com.commonsware.cwac:wakeful:1.0.+'
    compile 'org.jsoup:jsoup:1.8.3'
    compile 'jp.wasabeef:recyclerview-animators:1.2.2'
    compile "com.android.support:customtabs:23.1.1"
    compile 'com.anjlab.android.iab.v3:library:1.0.28'
    compile 'com.jakewharton:butterknife:7.0.1'
    compile 'com.kogitune:pre-lollipop-activity-transition:1.0.0'
    compile ('com.turhanoz.android:reactivedirectorychooser:0.0.12@aar'){
        transitive=true
    }
    compile 'xyz.danoz:recyclerviewfastscroller:0.1.3'
    compile 'com.github.nbsp-team:MaterialFilePicker:fc275cba7b'
    compile 'com.squareup.okhttp3:okhttp:3.1.2'
    compile 'com.github.DASAR:ShiftColorPicker:v0.5@aar'
    compile 'io.realm:realm-android:0.87.4'
    compile 'com.android.support:palette-v7:23.1.1'

}
